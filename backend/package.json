{
  "name": "refactorforge-backend",
  "version": "1.0.0",
  "description": "RefactorForge Backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist .tsbuildinfo && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\""
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.478.0",
    "@aws-sdk/credential-providers": "^3.478.0",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@google-cloud/secret-manager": "^5.0.1",
    "@octokit/auth-token": "^6.0.0",
    "@octokit/rest": "^22.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "lru-cache": "^10.2.0",
    "morgan": "^1.10.0",
    "p-limit": "^5.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "jest": "^30.1.1",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
